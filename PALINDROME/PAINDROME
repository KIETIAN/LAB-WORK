#include <stdio.h>
#include<string.h>
void main() {
    char str[20];
    int l,t,f=0,i;
    gets(str);
    for(l=0;str[l]!='\0';l++);
    t=l-1;
    for(i=0;i<=l;i++,t--)
    {
        if(str[i]!=str[t])
        {
            printf("it is not a palindrom");
            f=1;
            break;
        }
    } 
if(f==0)
        {
            printf("palindrom");
        }
    }

  
